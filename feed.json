---
---
[   
  {% for post in site.posts %}
    {% unless post.link %}
      {
        "url": "{{post.url}}",
        "title": {{post.title | jsonify}},
        "content": {{post.content | jsonify}},
        "keywords": "{{post.keywords}}",
        "created": "{{post.date | date: "%B %e, %Y"}}",
        "updated": "{{post.last_modified_at | date: "%B %e, %Y"}}"
      }{% unless forloop.last %},{% endunless %}
    {% endunless %}
  {% endfor %}
]